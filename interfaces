source /etc/network/interfaces.d/*

# The loopback network interface
auto lo
iface lo inet loopback

# The primary network interface
allow-hotplug enp0s3
iface enp0s3 inet dhcp

#Vizsgaremek network interface
# VLAN 10
iface enp0s8.10 inet static
  address 10.0.0.2/27
  gateway 10.0.0.1
  dns-domain dc1.docnetic.net.local
  dns-nameservers 10.0.0.66 8.8.8.8 4.4.4.4 1.1.1.1 1.0.0.1
  dns-search docnetic.net.local
  vlan_raw_device enp0s8

# VLAN 20
iface enp0s8.20 inet static
  address 10.0.0.35/27
  gateway 10.0.0.33
  dns-domain dc1.docnetic.net.local
  dns-nameservers 10.0.0.66 8.8.8.8 4.4.4.4 1.1.1.1 1.0.0.1
  dns-search docnetic.net.local
  vlan_raw_device enp0s8

# VLAN 50
iface enp0s8.50 inet static
  address 10.0.0.66/29
  gateway 10.0.0.65
  dns-domain dc1.docnetic.net.local
  dns-nameservers 10.0.0.66 8.8.8.8 4.4.4.4 1.1.1.1 1.0.0.1
  dns-search docnetic.net.local
  vlan_raw_device enp0s8
